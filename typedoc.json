{
  "entryPoints": [
    "src/index.ts"
  ],
  "out": "docs",
  "name": "WhatsApp Bot Framework",
  "excludePrivate": true,
  "excludeInternal": true,
  "plugin": [
    "typedoc-plugin-markdown"
  ],
  "theme": "default",
  "exclude": [
    "**/node_modules/**",
    "**/*.spec.ts"
  ],
  "readme": "README.md",
  "categorizeByGroup": true,
  "navigationLinks": {
    "Table of Contents": "/"
  },
  "sidebarLinks": {
    "API Documentation": "modules"
  }
}
